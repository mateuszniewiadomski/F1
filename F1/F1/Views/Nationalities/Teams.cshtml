@model F1.ViewModels.NationalityViewModel

@{
    ViewData["Title"] = "Teams from nation";
}

<h1>Teams from nation</h1>

<div>
    <h4>  @Html.DisplayFor(model => model.Nationality.NationName)</h4>
    <hr />
    <div>
        <div>
            @foreach (var item in Model.Teams)
            {
                @Html.Partial("_TeamsPartial", item);
                <hr />
            }
        </div>
    </div>
</div>
<div>
    <a type="button" class="btn btn-outline-primary" asp-action="Index">Back to List</a>
</div>